{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "jsx": "react-jsx",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "noUnusedLocals": false,
        "allowSyntheticDefaultImports": true,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "types": ["node", "webpack-env", "googlemaps", "markerclustererplus"],
        "typeRoots": ["./types"],
        "baseUrl": "src"
    },
    "include": ["src", "src/views/Dashboard/chart-data", "src/views/Dashboard/chart-data"]
}
